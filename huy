Trial:AddSection("Awakening")

Trial:AddToggle({
    Name = "Auto Training",
    Flag = "Trial/AutoFarmActiveV4",
    Description = "<font color='rgb(0, 255, 0)'>Farm Bones & V4 Active</font>",
    Default = false,
    Callback = function(v)
        _G.BoneFarmV4 = v
        hasTeleportedToGraveIsland = false
    end
})

spawn(function()
    while task.wait() do
        if _G.BoneFarmV4 then
            pcall(function()
                -- Auto Activate V4
                local char = game.Players.LocalPlayer.Character
                if char:FindFirstChild("RaceEnergy") and char.RaceEnergy.Value >= 1 and char:FindFirstChild("RaceTransformed") and not char.RaceTransformed.Value then
                    local args = { [1] = true }
                    game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Awakening").RemoteFunction:InvokeServer(unpack(args))
                end

                -- Auto Farm Bones
                if not hasTeleportedToGraveIsland then
                    -- Change the teleport position to Grave Island
                    Tween(CFrame.new(-9480.80762, 142.130661, 5566.37305, -0.00655503059, 4.5295423e-8, -0.999978542, 2.0492047e-8, 1, 4.5162068e-8, 0.999978542, -2.0195568e-8, -0.00655503059))
                    hasTeleportedToGraveIsland = true
                    task.wait(1)
                end

                local mobs = { "Reborn Skeleton", "Living Zombie", "Posessed Mummy", "Demonic Soul" }
                local target = nil

                for _, mobName in ipairs(mobs) do
                    for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                        if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            target = enemy
                            break
                        end
                    end
                    if target then break end
                end

                if target then
                    AutoHaki()
                    EquipTool(_G.SelectWeapon or SelectWeapon)
                    target.HumanoidRootPart.CanCollide = false
                    target.Humanoid.WalkSpeed = 0
                    if target:FindFirstChild("Head") then
                        target.Head.CanCollide = false
                    end
                    Tween(target.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0))
                    AttackNoCoolDown()
                else
                    task.wait(1)
                end
            end)
        end
    end
end)


