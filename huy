local hasTweened = false

Main:AddToggle({
    Name = "Auto Farm Cake Island Mobs",
    Flag = "Main/AutoFarmCakeIslandMobs",
    Description = "Farms all mobs in Cake Island",
    Default = false,
    Callback = function(Value)
        _G.AutoFarmCakeIsland = Value
        hasTweened = false -- reset so it teleports once when toggled back on
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoFarmCakeIsland then
            pcall(function()
                local mobs = {
                    "Baking Staff",
                    "Head Baker",
                    "Cookie Crafter",
                }

                if not hasTweened then
                    Tween(CFrame.new(-2021, 123, -12194)) -- tp once
                    hasTweened = true
                end

                for _, mobName in ipairs(mobs) do
                    if not _G.AutoFarmCakeIsland then break end

                    repeat
                        local mobFound = false
                        for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                                mobFound = true
                                AutoHaki()
                                EquipTool(_G.SelectWeapon or SelectWeapon)
                                enemy.HumanoidRootPart.CanCollide = false
                                enemy.Humanoid.WalkSpeed = 0
                                if enemy:FindFirstChild("Head") then enemy.Head.CanCollide = false end
                                Tween(enemy.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0))
                                AttackNoCoolDown()
                                break
                            end
                        end
                        task.wait()
                    until not mobFound or not _G.AutoFarmCakeIsland
                end
            end)
        end
    end
end)


Main:AddToggle({
    Name = "Auto Summon Cake Prince",
    Flag = "Main/AutoSummonCakePrince",
    Description = "Auto Summon Cake Prince",
    Default = true,
    Callback = function(Value)
        _G.SpawnCakePrince = Value
    end
})

spawn(function()
    while wait() do
        if _G.SpawnCakePrince then
            pcall(function()
                -- Assuming "CakePrinceSpawner" is the object that spawns the Cake Prince
                local spawnData = {
                    [1] = "CakePrinceSpawner",
                    [2] = true -- This will trigger the spawn
                }
                
                -- You may need to change this to the actual code that triggers the spawn
                local spawner = game.Workspace:FindFirstChild("CakePrinceSpawner")
                if spawner then
                    -- Call the function or event to spawn the Cake Prince
                    spawner:Fire(spawnData) -- Replace with actual method to spawn
                end
            end)
        end
    end
end)




-- Player Window

local playerList = {}
for _, player in pairs(game:GetService("Players"):GetPlayers()) do
    table.insert(playerList, player.Name)
end

Player:AddSection("Combat")

Player:AddDropdown({
    Name = "Select Player",
    Flag = "Player/SelectPlayers",
    Description = "Choose a player to target",
    Options = playerList,
    Callback = function(option)
        _G.SelectPly = option
    end
})

-- Toggle for Auto Kill Player
Player:AddToggle({
    Name = "Auto Kill Player",
    Flag = "Player/AutoKillPlayer",
    Description = "Teleports to selected player",
    Default = false,
    Callback = function(value)
        _G.TeleportPly = value
    end
})

-- Real-time tween follow loop
spawn(function()
    while task.wait(0.1) do
        pcall(function()
            if _G.TeleportPly and _G.SelectPly and game.Players:FindFirstChild(_G.SelectPly) then
                local target = game.Players[_G.SelectPly]
                if target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                    AutoHaki()
                    Tween(target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)) -- optional offset
                end
            end
        end)
    end
end)



Player:AddToggle({
    Name = "Spectate Player",
    Flag = "Player/SpectatePlayer",
    Description = "Enable or disable spectating",
    Default = false,
    Callback = function(Value)
        _G.SpectatePlayer = Value
        if Value then
            SpectatePlayer()
        else
            StopSpectating()
        end
    end
})


_G.Spectating = false
_G.SelectPly = nil

function SpectatePlayer()
    local localPlayer = game:GetService("Players").LocalPlayer
    local camera = game:GetService("Workspace").Camera

    if not _G.SelectPly or _G.SelectPly == "" then
        warn("No player selected!")
        StopSpectating()
        return
    end

    local targetPlayer = game:GetService("Players"):FindFirstChild(_G.SelectPly)
    if not targetPlayer or not targetPlayer.Character then 
        warn("Invalid player selected or player not in-game!")
        StopSpectating()
        return
    end

    camera.CameraSubject = targetPlayer.Character:FindFirstChildOfClass("Humanoid")
    _G.Spectating = true

    local connection
    connection = targetPlayer.CharacterRemoving:Connect(function()
        if _G.Spectating then
            StopSpectating()
            connection:Disconnect()
        end
    end)

    print("Now spectating:", _G.SelectPly)
end

function StopSpectating()
    local localPlayer = game:GetService("Players").LocalPlayer
    local camera = game:GetService("Workspace").Camera
    camera.CameraSubject = localPlayer.Character:FindFirstChildOfClass("Humanoid")
    _G.Spectating = false
    print("Stopped spectating.")
end


Player:AddSection("Player")

Player:AddToggle({
    Name = "NoClip",
    Flag = "Player/NoClip",
    Description = "Walk through walls",
    Default = true,
    Callback = function(Value)
        _G.NoClip = Value

        if _G.NoClip and not _G.NoClipLoop then
            _G.NoClipLoop = task.spawn(function()
                while _G.NoClip do
                    local char = game.Players.LocalPlayer.Character
                    if char and char:FindFirstChild("HumanoidRootPart") then
                        for _, part in pairs(char:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                            end
                        end
                    end
                    task.wait(0.1)
                end
                _G.NoClipLoop = nil
            end)
        end

        if not _G.NoClip and _G.NoClipLoop then
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                for _, part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
        end
    end
})


Player:AddToggle({
    Name = "Walk on Water",
    Flag = "Player/WalkonWater",
    Description = "",
    Default = true,
    Callback = function(Value)
        _G.WalkonWater = Value
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.WalkonWater then
                game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000, 112, 1000)
            else
                game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000, 80, 1000)
            end
        end)
    end
end)

-- Add this function to format numbers (replacement for v20)
function FormatNumber(number)
    return math.floor(number + 0.5)
end


Player:AddSlider({
    Name = "Walkspeed",
    Flag = "Player/SpeedMultiplier",
    Description = "",
    Range = {1, 300},
    Suffix = "",
    CurrentValue = 300,
    Increment = 1,
    Callback = function(Value)
        _G.SpeedMultiplier = Value
    end,
})

-- Initialize global variables
_G.SpeedBoost = false
_G.SpeedMultiplier = 300

-- Velocity-based speed boost
task.spawn(function()
    while true do
        if _G.SpeedBoost then
            pcall(function()
                local char = game.Players.LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Humanoid") then
                    local moveDir = char.Humanoid.MoveDirection
                    if moveDir.Magnitude > 0 then
                        local boost = moveDir.Unit * (_G.SpeedMultiplier * 3.5)
                        boost = Vector3.new(boost.X, char.HumanoidRootPart.Velocity.Y, boost.Z)
                        char.HumanoidRootPart.Velocity = boost
                    end
                end
            end)
        end
        task.wait()
    end
end)

Player:AddToggle({
    Name = "Enable Walkspeed",
    Flag = "Player/SpeedBoost",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.SpeedBoost = Value
        if not Value then
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                char.HumanoidRootPart.Velocity = Vector3.new(0, char.HumanoidRootPart.Velocity.Y, 0)
            end
        end
    end,
})




