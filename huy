Main:AddSection("Cake")

Main:AddToggle({
    Name = "Auto Farm Dough King",
    Flag = "Main/AutoFarmDoughKing",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoFarmDoughKing = Value
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoFarmDoughKing then
            pcall(function()
                local mobs = {
                    "Dough King"
                }

                -- Teleport once to area near Dough King
                if not hasTweened then
                    Tween(CFrame.new(-2021, 123, -12194)) -- adjust if needed
                    hasTweened = true
                end

                for _, mobName in ipairs(mobs) do
                    if not _G.AutoFarmDoughKing then break end

                    repeat
                        local mobFound = false
                        for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                                mobFound = true
                                AutoHaki()
                                EquipTool(_G.SelectWeapon or SelectWeapon)
                                enemy.HumanoidRootPart.CanCollide = false
                                enemy.Humanoid.WalkSpeed = 0
                                if enemy:FindFirstChild("Head") then enemy.Head.CanCollide = false end
                                Tween(enemy.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0))
                                AttackNoCoolDown()
                                break
                            end
                        end
                        task.wait()
                    until not mobFound or not _G.AutoFarmDoughKing
                end
            end)
        else
            hasTweened = false
        end
    end
end)


