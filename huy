    end
})

Main:AddSection("Tiki")

Main:AddToggle({
    Name = "Auto Farm Tyrant of the Skies",
    Flag = "Main/AutoFarmTyrantOfTheSkies",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoFarmTyrant = Value
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoFarmTyrant then
            pcall(function()
                local mobName = "Tyrant of the Skies"

                -- Teleport to the area and tween to main spot
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(5756, 610, -282))
                Tween(CFrame.new(-16224, 9, 439))

                repeat
                    local mobFound = false
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            mobFound = true
                            AutoHaki()
                            EquipTool(_G.SelectWeapon or SelectWeapon)
                            enemy.HumanoidRootPart.CanCollide = false
                            enemy.Humanoid.WalkSpeed = 0
                            if enemy:FindFirstChild("Head") then enemy.Head.CanCollide = false end
                            Tween(enemy.HumanoidRootPart.CFrame * CFrame.new(0, 45, 0))
                            AttackNoCoolDown()
                            break
                        end
                    end
                    task.wait()
                until not mobFound or not _G.AutoFarmTyrant
            end)
        end
    end
end)






