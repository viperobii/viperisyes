Discord:AddDiscordInvite({
    Name = "nova Hub | Community",
    Description = "Join our discord community to receive information about the next update",
    Logo = "rbxassetid://120931314112734", -- Check if this ID is correct and exists
    Invite = "https://discord.gg/brrYwzpWg7"
})




Main:AddDropdown({
    Name = "UI Scale",
    Flag = "Main/UIScale",
    Default = "bigger",
    Options = { "small", "medium", "large", "bigger" },
    Callback = function(option)
        local scales = {
            small = 950,   -- smaller UI
            medium = 750,  -- slightly smaller
            large = 550,    -- default size, no change
            bigger = 450    -- larger UI
        }
        if scales[option] then
            redzlib:SetScale(scales[option])
        end
    end
})


Main:AddDropdown({
    Name = "Logo Size",
    Flag = "Main/LogoSize",  -- Set a unique flag for easy reference
    Default = "Medium",  -- Set the default size
    Options = { "Small", "Medium", "Large" },  -- The options for logo size
    Callback = function(option)
        -- Set the size based on the selected option
        if option == "Small" then
            v23.Size = UDim2.new(0, 30, 0, 30)  -- Small size
        elseif option == "Medium" then
            v23.Size = UDim2.new(0, 50, 0, 50)  -- Medium size
        elseif option == "Large" then
            v23.Size = UDim2.new(0, 100, 0, 100)  -- Large size
        end
    end
})


Main:AddSection("Farm")

Main:AddToggle({
    Name = "Auto Farm Level",
    Flag = "Main/AutoFarmLevel",
    Description = "Fast Farm With Dodge Skill Mobs",
    Default = false,
    Callback = function(Value)
        _G.AutoLevel = Value
        if not Value then
            wait()
            -- Ensure player is above the water level
            local player = game:GetService("Players").LocalPlayer
            local playerPosition = player.Character.HumanoidRootPart.Position
            if playerPosition.Y <= 0 then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(playerPosition.X, 5, playerPosition.Z)
            end
            Tween(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoLevel then
            pcall(function()
                CheckLevel()

                local player = game:GetService("Players").LocalPlayer
                local questGui = player.PlayerGui.Main.Quest

                if not string.find(questGui.Container.QuestTitle.Title.Text, NameMon) or not questGui.Visible then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                    Tween(CFrameQ)

                    -- Ensure player stays above water after tweening
                    local playerPosition = player.Character.HumanoidRootPart.Position
                    if playerPosition.Y <= 0 then
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(playerPosition.X, 5, playerPosition.Z)
                    end

                    if (CFrameQ.Position - player.Character.HumanoidRootPart.Position).Magnitude <= 5 then
                        wait(0.5) -- Small delay before accepting quest
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    end
                else
                    -- Check if there are mobs to farm
                    local foundMob = false
                    
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            if string.find(enemy.Name, Ms) then
                                foundMob = true
                                
                                -- Target and attack the mob
                                AutoHaki()
                                EquipTool(SelectWeapon)
                                
                                enemy.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                                enemy.HumanoidRootPart.Transparency = 1
                                enemy.Humanoid.JumpPower = 0
                                enemy.Humanoid.WalkSpeed = 0
                                enemy.HumanoidRootPart.CanCollide = false
                                
                                local targetPos = enemy.HumanoidRootPart.Position + Vector3.new(0, 30, 0)
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
                                
                                -- Attack
                                AttackNoCoolDown()
                                bringmob = true
                                
                                wait(0.1)
                                break -- Focus on one mob at a time
                            end
                        end
                    end
                    
                    -- If no mobs found, go to spawn location
                    if not foundMob then
                        bringmob = false
                        -- Go to mob spawn location
                        Tween(CFrameMon)

                        -- Ensure player stays above water during movement
                        local playerPosition = player.Character.HumanoidRootPart.Position
                        if playerPosition.Y <= 0 then
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(playerPosition.X, 5, playerPosition.Z)
                        end

                        wait(0.5)
                    end
                end
            end)
        end
    end
end)

-- AUTO FARM NEAREST MOB TOGGLE
Main:AddToggle({
    Name = "Auto Farm Nearest Mob",
    Flag = "Main/AutoFarmNearestMob",
    Description = "Nearest Mob",
    Default = false,
    Callback = function(Value)
        _G.AutoNear = Value
        if not Value then bringmob = false end
    end
})

-- AUTO FARM NEAREST MOB LOOP
spawn(function()
    while task.wait() do
        if _G.AutoNear then
            pcall(function()
                local Closest = nil
                local ClosestDistance = math.huge
                for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                        local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
                        if dist < ClosestDistance then
                            Closest = enemy
                            ClosestDistance = dist
                        end
                    end
                end

                if Closest then
                    repeat
                        task.wait(_G.Fast_Delay or 0.1)
                        AutoHaki()
                        EquipTool(SelectWeapon)
                        Tween(CFrame.new(Closest.HumanoidRootPart.Position + Vector3.new(0, 20, 0)))
                        AttackNoCoolDown()
                        if Closest:FindFirstChild("HumanoidRootPart") then
                            Closest.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            Closest.HumanoidRootPart.Transparency = 1
                            Closest.HumanoidRootPart.CanCollide = false
                        end
                        if Closest:FindFirstChild("Humanoid") then
                            Closest.Humanoid.WalkSpeed = 0
                            Closest.Humanoid.JumpPower = 0
                        end
                        bringmob = true
                    until not _G.AutoNear or Closest.Humanoid.Health <= 0 or not Closest.Parent
                    bringmob = false
                end
            end)
        end
    end
end)

-- AUTO COLLECT CHEST TOGGLE
Main:AddSection("Chest")

Main:AddToggle({
    Name = "Auto Farm Chest",
    Flag = "Main/AutoCollectChest",
    Description = "Collect All Chest",
    Default = false,
    Callback = function(Value)
        _G.AutoCollectChest = Value
    end
})

spawn(function()
    while wait() do
        if _G.AutoCollectChest then
            pcall(function()
                local player = game:GetService("Players").LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local rootPos = character:GetPivot().Position

                local chestTag = game:GetService("CollectionService"):GetTagged("_ChestTagged")
                local closestDistance = math.huge
                local closestChest = nil

                for _, chest in pairs(chestTag) do
                    local dist = (chest:GetPivot().Position - rootPos).Magnitude
                    if not chest:GetAttribute("IsDisabled") and dist < closestDistance then
                        closestDistance = dist
                        closestChest = chest
                    end
                end

                if closestChest then
                    Tween2(CFrame.new(closestChest:GetPivot().Position))
                end
            end)
        end
    end
end)

Main:AddToggle({
    Name = "Auto Farm Chest (Instant TP)",
    Flag = "Main/ChestBypass",
    Description = "Automatically teleports to the nearest chest instantly.",
    Default = false,
    Callback = function(Value)
        _G.ChestBypass = Value
    end
})

spawn(function()
    while task.wait() do
        if _G.ChestBypass then
            local Players = game:GetService("Players")
            local LocalPlayer = Players.LocalPlayer
            local CollectionService = game:GetService("CollectionService")
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local startTime = tick()

            while (tick() - startTime) < 4 do
                Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                local playerPos = Character:GetPivot().Position
                local taggedChests = CollectionService:GetTagged("_ChestTagged")

                local closestDist = math.huge
                local closestChest = nil

                for i = 1, #taggedChests do
                    local chest = taggedChests[i]
                    local dist = (chest:GetPivot().Position - playerPos).Magnitude
                    if not chest:GetAttribute("IsDisabled") and dist < closestDist then
                        closestDist = dist
                        closestChest = chest
                    end
                end

                if closestChest then
                    local chestPos = closestChest:GetPivot()
                    BTPZ(chestPos) -- INSTANT TP
                    task.wait(0.2)
                else
                    break
                end
            end

            if LocalPlayer.Character then
                LocalPlayer.Character:BreakJoints()
                LocalPlayer.CharacterAdded:Wait()
            end
        end
    end
end)

Main:AddSection("Bones");


local hasTeleportedToBoneIsland = false

Main:AddToggle({
    Name = "Auto Farm Bones",
    Flag = "Main/AutoFarmBones",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoFarmBones = Value
        if Value == false then
            hasTeleportedToBoneIsland = false -- Reset when turning off
        end
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoFarmBones then
            pcall(function()
                -- First teleport to Bone Island once when enabled
                if not hasTeleportedToBoneIsland then
                    Tween(CFrame.new(-9480.80762, 142.130661, 5566.37305, -0.00655503059, 4.5295423e-8, -0.999978542, 2.0492047e-8, 1, 4.5162068e-8, 0.999978542, -2.0195568e-8, -0.00655503059))
                    hasTeleportedToBoneIsland = true
                    task.wait(1) -- Wait for teleport to complete
                end
                
                local mobs = {
                    "Reborn Skeleton",
                    "Living Zombie",
                    "Posessed Mummy",
                    "Demonic Soul"
                }
                
                -- Scan for any Bone mob in the entire workspace
                local foundMob = false
                local targetMob = nil
                
                for _, mobName in ipairs(mobs) do
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and 
                           enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            targetMob = enemy
                            foundMob = true
                            break
                        end
                    end
                    if foundMob then break end
                end
                
                -- If we found a mob, attack it
                if foundMob and targetMob then
                    AutoHaki()
                    EquipTool(_G.SelectWeapon or SelectWeapon)
                    targetMob.HumanoidRootPart.CanCollide = false
                    targetMob.Humanoid.WalkSpeed = 0
                    if targetMob:FindFirstChild("Head") then targetMob.Head.CanCollide = false end
                    Tween(targetMob.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0))
                    AttackNoCoolDown()
                end
                
                -- Brief wait if no mob was found to prevent excessive looping
                if not foundMob then
                    task.wait(1)
                end
            end)
        end
    end
end)


Main:AddToggle({
    Name = "Auto Roll Bones",
    Flag = "Main/AutoRollBones",
    Description = "Auto Spin Bone",
    Default = false,
    Callback = function(Value)
        _G.AutoRollBones = Value
        if Value then
            spawn(function()
                while _G.AutoRollBones do
                    pcall(function()
                        local args = {
                            [1] = "Bones",
                            [2] = "Buy",
                            [3] = 1,
                            [4] = 1
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end)
                    wait(1)  -- Adjust the wait time if needed
                end
            end)
        end
    end
})


